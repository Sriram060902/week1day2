
let browserName = "chrome"; // You can change this to "firefox", "edge", etc.


if (browserName === "chrome") {
  console.log("Launching Chrome browser...");
} else {
  console.log("Launching default browser...");
}


let testType = "sanity"; // You can change this to "smoke", "regression", etc.


switch (testType) {
  case "smoke":
    console.log("Running Smoke Tests...");
    break;

  case "sanity":
    console.log("Running Sanity Tests...");
    break;

  case "regression":
    console.log("Running Regression Tests...");
    break;

  default:
    console.log("Running Default Smoke Tests...");
    break;
}
